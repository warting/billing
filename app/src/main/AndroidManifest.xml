<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PerfectAndroidLibraryTemplate">
        <activity
            android:name="se.warting.sampleapp.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.PerfectAndroidLibraryTemplate.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="se.warting.sampleapp.compose.ComposeActivity"
            android:exported="false" />
        <activity
            android:name="se.warting.sampleapp.advance.AdvanceActivity"
            android:exported="false" />
    </application>

</manifest>